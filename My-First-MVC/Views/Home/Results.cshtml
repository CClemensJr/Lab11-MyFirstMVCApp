@model List<My_First_MVC.Models.TimePerson>

<!DOCTYPE html>
<html>
<head>
    <title>Results</title>
    <link href="~/StyleSheet.css" rel="stylesheet" />
</head>
<body>
    <h1>Persons of the Years</h1>
    <h2>Years</h2>

    <table>
        <thead>
            <tr>
                <th>Year</th>
                <th>Name</th>
                <th>Honor</th>
                <th>Country</th>
                <th>Date of Birth</th>
                <th>Expiration Date</th>
                <th>Title</th>
                <th>Category</th>
                <th>What They Did:</th>
            </tr>
        </thead>
        <tbody>

        </tbody>
    </table>

    @foreach (var item in @Model)
    {
        <h3>@item.Year</h3>
        <h3>@item.Name</h3>
        <h3>@item.Honor</h3>
        <h3>@item.Country</h3>
        <h3>@item.BirthYear</h3>
        <h3>@item.DeathYear</h3>
        <h3>@item.Title</h3>
        <h3>@item.Category</h3>
        <h3>@item.Context</h3>
    }

</body>
</html>
